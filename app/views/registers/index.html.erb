<h1>登録者一覧</h1>

<table>
  <tr>
    <th>ＩＤ番号</th>
    <th>メール</th>
    <th>削除</th>
  </tr>
<% @users.each do |user| %>
  <tr>
    <td><%= user[:uid] %></td>
    <td><%= user[:mail] %></td>
    <td><%= link_to '削除', "/users/#{user.id}", method: :delete %></td>
  </tr>
<% end %>
</table>

<%= link_to "新規登録", new_register_path %>